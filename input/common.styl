    .public-header_public-nav
        .container
            display flex
            align-items flex-end
            justify-content space-between
            height 95px
            @media(max-width 991px)
                position relative
            @media(max-width 767px)
                height 60px
                align-items center
            &:after
            &:before
                display none
        .public-header_public_nav-left
            padding-bottom 20px
