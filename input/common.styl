                @media(max-width 767px)
                    height 100%
                img
                    max-width 210px
                    @media(max-width 767px)
                        max-height 100%
        .public-header_public_nav-right
            margin 0
            .mobile-menu-wrap
                display none
                display flex
                min-width 47px
                padding-bottom 20px
                @media(max-width 900px)
                    display flex
                @media(max-width 767px)
                    padding-bottom 0
                .mobile-menu
                    display flex
                    max-width 47px
                    max-height 47px
                    align-items center
                    justify-content center
                    width 8vw
                    height 8vw
                    transition 0.3s
                    background transparent
                    &.active
                        position relative
                        left 0
                        z-index 1
                        background #ffffff
                        span
                            background rgba(0,0,0,0)
                            &:after
                                bottom 0
                                transform rotateZ(45deg)
                                transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
                            &:before
                                top 0
                                transform rotateZ(-45deg)
                                transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
                    span
                        position relative
                        width 70%
                        height 2px
                        background #000000
                        transition 0.3s
                        &:after
                            content ""
                            position absolute
                            bottom -3vw
                            left 0
                            right 0
                            border-top 2px solid #000
                            transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                        &:before
                            content ''
                            position absolute
                            top -3vw
                            left 0
                            right 0
                            border-top 2px solid #000
                            transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                        &:after
                            @media(min-width 500px)
                                bottom -13px
                        &:before
                            @media(min-width 500px)
                                top -13px
            .list
                @media(min-width 992px)
                    display flex!important
                @media(max-width 991px)
                    position absolute
                    left 50%
                    right 0
                    top 100%
                    background #ffffff
                    display block
                    z-index 10
                    display none
                    max-width 768px
                    width 100%
                    transform translateX(-50%)
                    border-top 1px solid #dadada
                    border-bottom 1px solid #dadada
                @media(max-width 767px)
                    left 0
                    transform translateX(0)
                .menu-root
                    position relative
                    line-height 1
                    display block
                    margin 0 20px
                    padding 20px 10px
                    font-size 18px
                    color #333333
                    font-weight 500
                    &.active
                    &:hover
                        &:after
                        &:before
                            opacity 1
                    &:after
                        content ''
                        position absolute
                        bottom 0
                        left 0
                        right 0
                        border-top 3px solid #1252a3
                        opacity 0
                        transition 0.3s
                    &:before
                        content ''
                        position absolute
                        left 50%
                        bottom 3px
                        transform translateX(-4px)
                        border 4px solid transparent
                        border-bottom-color #1252a3
                        opacity 0
                        transition 0.3s
                    .root-a
                        display block
                        width 100%
                        height 100%
                        cursor pointer
                    .child-menu
                        position absolute
                        display none
                        right 0
                        top 100%
                        box-shadow 1px 1px 13px rgba(#040000,0.17)
                        background #ffffff
                        width 600px
                        z-index 10
                        padding-bottom 30px
                        box-sizing border-box
                        padding-left 50px
                        @media(max-width 991px)
                            position static
                            padding 0 20px
                            width 100%
                            box-sizing border-box
                            box-shadow none
                        &.show
                            display block
                        h3
                            padding 30px 0 10px
                            font-size 18px
                            color #1357ac
                        a
                            display block
                            font-size 16px
                            color #666666
                            line-height 2
                            margin 1px 0
                        .item
                            width 180px
                            @media(max-width 991px)
                                width 100%
                                float none
                        .link-group
                            min-height 80px
                            min-width 1px


